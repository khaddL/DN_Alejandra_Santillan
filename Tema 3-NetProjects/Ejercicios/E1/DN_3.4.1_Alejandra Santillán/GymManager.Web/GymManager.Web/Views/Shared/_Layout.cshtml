<!doctype html>
<html lang="en" class="h-100">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Mark Otto, Jacob Thornton, and Bootstrap contributors">
    <meta name="generator" content="Hugo 0.88.1">
    <title>@ViewBag.Title</title>

    <link rel="canonical" href="https://getbootstrap.com/docs/5.1/examples/sticky-footer-navbar/">



    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="~/Bootstrap/css/bootstrap.min.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Lobster&family=Roboto+Mono:wght@100&display=swap" rel="stylesheet">

    <!-- Favicons -->

    <meta name="theme-color" content="#7952b3">


    <style>
        .bd-font-familyholder-img {
            font-size: 1.125rem;
            text-anchor: middle;
        .containercontainerontainer   -webkit-user-select: none;
            -moz-user-select: none;
            user-select: none;
        }

     .container  @@media (min-width: 768px) {
            .bd-placeholder-img-lg {
                font-size: 3.5rem;
            }
        }

        main > .container {
            padding: 100px 20px 0;
        }

        .navbar ul li a {
            font-family: 'Lobster', cursive;
            font-size: 20px;
        }
        .container-fluid {
            font-family: 'Lobster', cursive;
            font-size: 35px;
        }

        .container h1 {
            text-align:center;
            font-family:'Lucida Sans';
        }
        
        .container form{
            text-align:center;
            align-content:center;
        }
      
        .container div label{
        
        display: flex;
        justify-content: center;
        
        }
        .container div input[type=text]{
        
        width: 100%;
        height: 30px;
        margin: 0.5rem;
        text-align: center;
        }
        

    </style>

    @RenderSection("Head", false)
    <!-- Custom styles for this template -->

</head>
<body class="d-flex flex-column h-100">
    
    <header>
        <!-- Fixed navbar -->
         <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-danger">
            <div class="container-fluid">
                <div class="container" href="#" style="padding-top: 0px;width: 324px;"><img src="~/Images/gym.jpg" width="70"/>  Gym Manager</div>

                <div class="collapse navbar-collapse" style="align-content:center" id="navbarCollapse">
                    <ul class="navbar-nav me-auto mb-2 mb-md-0">
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="#" font-family="">Home</a>
                        </li>
                         <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="#" font-family="">Administration</a>
                        </li>
                         <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="#" font-family="">Store</a>
                        </li>
                         <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="#" font-family="">Members</a>
                        </li>
                         <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="#" font-family="">Invoicing</a>
                        </li>
                         <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="#" font-family="">Reports</a>
                        </li>
                    </ul>
                    
                       @RenderSection("HeaderRight", false)


                </div>
            </div>
        </nav>
    </header>

    <!-- Begin page content -->
    <main class="flex-shrink-0">
        @RenderBody()
    </main>
    <footer class="footer mt-auto py-3 bg-light">
        <div class="container">
            <span class="text-muted">Place sticky footer content here.</span>
        </div>
    </footer>


                <!-- Modal -->
          
                    <div class="modal fade" tabindex="-1" id="contenido-modal">
                      <div class="modal-dialog">
                        <div class="modal-content">
                          <div class="modal-header">
                            <h5 class="modal-title">Welcome!!</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                          </div>
                          <div class="modal-body">
                            <p id="valueInput"> </p>
                          </div>
                          <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                          </div>
                        </div>
                      </div>
                    </div>

    <script src="~/Bootstrap/js/bootstrap.bundle.min.js"></script>
       @RenderSection("Scripts", false)

</body>
</html>
